{
    "name": "angularjs-webpack-cdn",
    "version": "0.0.1",
    "keywords": [
        "angular2",
        "webpack",
        "typescript"
    ],
    "author": "Ayush Walia <krjean@gmail.com>",
    "license": "MIT",
    "scripts": {
        "move-resources": "gulp default",
        "start": "npm run server:dev",
        "build": "npm run build:dev",
        "build:dev": "npm run move-resources && webpack --config config/webpack/webpack.dev.js --progress --profile",
        "server:dev": "npm run move-resources && webpack-dev-server --config config/webpack/webpack.dev.js --open --progress --profile --watch",
        "server:dev:hmr": "npm run server:dev -- --inline --hot",
        "build:prod": "npm run move-resources && webpack --config config/webpack/webpack.prod.js --progress --profile --bail",
        "build:aot:prod": "npm run move-resources && webpack --config config/webpack/webpack.prod.js  --progress --profile --bail",
        "server:prod": "npm run move-resources && webpack-dev-server --config config/webpack/webpack.prod.js --open --progress --profile --watch",
        "docs": "npm run typedoc -- --options typedoc.json ./src/",
        "typedoc": "typedoc",
        "webpack": "webpack",
        "watch-sass": "gulp watch-sass"
    },
